<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.www.activity.SettingActivity"
    android:orientation="vertical">

 <TextView
     android:id="@+id/textView5"
     style="@style/titleStyle"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:text="设置中心" />

 <com.example.www.view.SettingItemView
     xmlns:mobilesafe="http://schemas.android.com/apk/res/com.example.www.mobilesafe"
     android:id="@+id/siv_update"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     mobilesafe:destitle="自动更新设置"
     mobilesafe:deson="自动更新已开启"
     mobilesafe:desoff="自动更新已关闭"/>
 <com.example.www.view.SettingItemView
     xmlns:mobilesafe="http://schemas.android.com/apk/res/com.example.www.mobilesafe"
     android:id="@+id/siv_address"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     mobilesafe:destitle="电话归属地的显示设置"
     mobilesafe:deson="归属地显示已开启"
     mobilesafe:desoff="归属地显示已关闭"/>
</LinearLayout>
